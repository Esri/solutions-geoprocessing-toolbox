          <in_service_definition_draft></in_service_definition_draft>
          <out_service_definition></out_service_definition>
        </StageService_server>
        <!-- USD Uploads and publishes a service definition to a specified server -->
        <!-- http://resources.arcgis.com/en/help/main/10.2/index.html#//00540000001p000000 -->
        <UploadServiceDefinition_server>
          <in_sd_file></in_sd_file>
          <in_server></in_server>
          <in_service_name />
          <in_cluster />
          <in_folder_type />
          <in_folder />
          <in_startupType />
          <in_override />
          <in_my_contents />
          <in_public />
          <in_organization />
          <in_groups />
        </UploadServiceDefinition_server>
      </Processes>
    </MosaicDataset>
