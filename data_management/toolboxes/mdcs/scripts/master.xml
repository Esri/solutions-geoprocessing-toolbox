        </ExportMosaicDatasetItems>
        <!-- SMDI Splits mosaic dataset items that were merged together using Merge Mosaic Dataset Items -->
        <!-- http://resources.arcgis.com/en/help/main/10.2/index.html#//00170000018q000000 -->
        <SplitMosaicDatasetItems>
