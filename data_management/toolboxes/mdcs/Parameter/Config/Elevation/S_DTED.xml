<?xml version="1.0" ?><Application>
  <Name>Elevation_Project</Name>
  <Command>CM+AR+CC+BB+SP+CS+BO</Command>
  <Workspace>
    <WorkspacePath>MD</WorkspacePath>
    <Geodatabase>Sample1.gdb</Geodatabase>
      <MosaicDataset>
      <MosaicDatasetType>Source</MosaicDatasetType>
        <Name>S_GMTED2010</Name>
<SRS>PROJCS['WGS_1984_Web_Mercator_Auxiliary_Sphere',GEOGCS['GCS_WGS_1984',DATUM['D_WGS_1984',SPHEROID['WGS_1984',6378137.0,298.257223563]],PRIMEM['Greenwich',0.0],UNIT['Degree',0.0174532925199433]],PROJECTION['Mercator_Auxiliary_Sphere'],PARAMETER['False_Easting',0.0],PARAMETER['False_Northing',0.0],PARAMETER['Central_Meridian',0.0],PARAMETER['Standard_Parallel_1',0.0],PARAMETER['Auxiliary_Sphere_Type',0.0],UNIT['Meter',1.0]];$sSRS$</SRS>
     
        <AddRasters> 
         <AddRaster>	   
          <dataset_id>smd;$did$</dataset_id>
          <raster_type>DTED</raster_type>
          <Sources>
            <data_path>c:\Image_Mgmt_Workflows\elevation\data\GMTED2010</data_path>
          </Sources>
          <filter>*</filter>
         </AddRaster>
        </AddRasters> 
        <DefaultProperties>
          <max_num_per_mosaic>50</max_num_per_mosaic>
          <rows_maximum_imagesize>4000</rows_maximum_imagesize>
          <columns_maximum_imagesize>4000</columns_maximum_imagesize>
          <allowed_compressions>LZ77;NONE;LERC</allowed_compressions>
          <default_compression_type>LZ77</default_compression_type>
          <JPEG_quality>75</JPEG_quality>
	  <resampling_type>BILINEAR</resampling_type>
	  <LERC_Tolerance>0.01</LERC_Tolerance>
          <clip_to_footprints>CLIP</clip_to_footprints>
          <clip_to_boundary>CLIP</clip_to_boundary>
	  <color_correction>NOT_APPLY</color_correction>
	  <footprints_may_contain_nodata>FOOTPRINTS_MAY_CONTAIN_NODATA</footprints_may_contain_nodata>
	  <allowed_mensuration_capabilities>BASIC</allowed_mensuration_capabilities>
	  <default_mensuration_capabilities>BASIC</default_mensuration_capabilities>
	  <allowed_mosaic_methods>NorthWest;LockRaster;ByAttribute;Seamline;None</allowed_mosaic_methods>
          <default_mosaic_method>NorthWest</default_mosaic_method>
          <Order_field>#</Order_field>  
	  <order_base>0</order_base>
	  <sorting_order>Ascending</sorting_order>
	  <mosaic_operator>FIRST</mosaic_operator>
	  <blend_width>10</blend_width>
	  <view_point_x>300</view_point_x>
	  <view_point_y>300</view_point_y>
	  <max_num_per_mosaic>50</max_num_per_mosaic>
	  <cell_size_tolerance>999</cell_size_tolerance>
	  <cell_size>#</cell_size>
	  <metadata_level>BASIC</metadata_level>
      <transmission_fields>Name;MinPS;MaxPS;LowPS;HighPS;ProductName</transmission_fields>
	  <use_time>DISABLED</use_time>
	  <start_time_field>#</start_time_field>
	  <end_time_field>#</end_time_field>
	  <time_format>#</time_format>
	  <geographic_transform>#</geographic_transform>
	  <max_num_of_download_items>50</max_num_of_download_items>
	  <max_num_of_records_returned>2000</max_num_of_records_returned>
	  <data_source_type>ELEVATION</data_source_type>
	  <minimum_pixel_contribution>#</minimum_pixel_contribution>
	  <processing_templates>AspectNumericValues.rft.xml;AspectRGBColorMap.rft.xml;GreyScaleHillshade.rft.xml;ShadedRelief.rft.xml;SlopePercent.rft.xml</processing_templates>
	  <default_processing_template>None</default_processing_template>
        </DefaultProperties>
         <Table>
          <AddFields>AddFields</AddFields>
          <Fields>
            <Field>
              <Name>Dataset_ID</Name>
              <Type>TEXT</Type>
              <Length>32</Length>
            </Field>
	    <Field>
              <Name>BEST</Name>
              <Type>FLOAT</Type>
              <Length/>
            </Field>
            <Field>
              <Name>Source</Name>
              <Type>TEXT</Type>
              <Length>32</Length>
            </Field>
             <Field>
              <Name>LE90</Name>
              <Type>FLOAT</Type>
              <Length/>
            </Field>
            <Field>
              <Name>CE90</Name>
              <Type>FLOAT</Type>
              <Length/>
            </Field>
            <Field>
              <Name>DEM_Type</Name>
              <Type>SHORT</Type>
              <Length>4</Length>
            </Field>
            <Field>
              <Name>Date_Start</Name>
              <Type>DATE</Type>
              <Length/>
            </Field>
            <Field>
              <Name>Date_End</Name>
              <Type>DATE</Type>
              <Length/>
            </Field>
            <Field>
              <Name>Source_URL</Name>
              <Type>TEXT</Type>
              <Length>64</Length>
            </Field>
            <Field>
              <Name>VerticalDatum</Name>
              <Type>TEXT</Type>
              <Length>16</Length>
            </Field>
            <Field>
              <Name>Link2Metadata</Name>
              <Type>TEXT</Type>
              <Length>100</Length>
            </Field>
          </Fields>
         </Table>

	  <Processes>
	<CalculateCellSizeRanges>
			<where_clause/>
			<do_compute_min>MIN_CELL_SIZES</do_compute_min>
			<do_compute_max>MAX_CELL_SIZES</do_compute_max>
			<max_range_factor>10</max_range_factor>
			<cell_size_tolerance_factor>0.8</cell_size_tolerance_factor>
			<update_missing_only>UPDATE_ALL</update_missing_only>
		</CalculateCellSizeRanges>
		<CalculateStatistics>
          <x_skip_factor>10</x_skip_factor>
          <y_skip_factor>10</y_skip_factor>
          <ignore_values>#</ignore_values>
          <skip_existing>OVERWRITE</skip_existing>
          <area_of_interest>#</area_of_interest>
        </CalculateStatistics>
		<BuildPyramidsAndStatistics>
					<include_subdirectories>NONE</include_subdirectories>
					<build_pyramids>NONE</build_pyramids>
					<calculate_statistics>NO_STATISTICS</calculate_statistics>
					<BUILD_ON_SOURCE>NONE</BUILD_ON_SOURCE>
					<block_field>#</block_field>
					<estimate_statistics>NONE</estimate_statistics>
					<x_skip_factor>10</x_skip_factor>
					<y_skip_factor>1</y_skip_factor>
					<ignore_values>#</ignore_values>
					<pyramid_level>1</pyramid_level>
					<SKIP_FIRST>NONE</SKIP_FIRST>
					<resample_technique>NEAREST</resample_technique>
					<compression_type>DEFAULT</compression_type>
					<compression_quality>75</compression_quality>
					<skip_existing>SKIP_EXISTING</skip_existing>
				</BuildPyramidsAndStatistics>
		<BuildOverviews>
					<where_clause>#</where_clause>
					<define_missing_tiles>#</define_missing_tiles>
					<generate_overviews>GENERATE_OVERVIEWS</generate_overviews>
					<generate_missing_images>GENERATE_MISSING_IMAGES</generate_missing_images>
					<regenerate_stale_images>REGENERATE_STALE_IMAGES</regenerate_stale_images>
				</BuildOverviews>
        <BuildFootprint>
			<max_data_value>3000</max_data_value>
			<min_data_value>-100</min_data_value>
			<approx_num_vertices>25</approx_num_vertices>
			<shrink_distance>2</shrink_distance>
			<update_boundary>NO_BOUNDARY</update_boundary>
		</BuildFootprint>
        <BuildBoundary>
			<simplification_method>ENVELOPE</simplification_method>
		</BuildBoundary>
        <DefineMosaicDatasetNoData>
			<num_bands>1</num_bands>
			<bands_for_nodata_value>ALL_BANDS 0</bands_for_nodata_value>
		</DefineMosaicDatasetNoData>
        <SetStatistics>
			
			<data_type>ELEVATION</data_type>
			<stats_file>SourceMD.xml</stats_file>
	</SetStatistics>
         <AddIndex>
		<Index>
			<fields>@fields</fields>
			<index_name>@index_name</index_name>
			<unique>@unique</unique>
			<ascending>@ascending</ascending>
		</Index>
		<Index>
			<fields>@fields</fields>
			<index_name>@index_name</index_name>
			<unique>@unique</unique>
			<ascending>@ascending</ascending>
		</Index>
	</AddIndex>
        <CalculateValues>
		<CalculateValue>
	 <FieldName>ProductName</FieldName>
 		  <Expression>&quot;GMTED_2010&quot;</Expression>
	  </CalculateValue>
         <CalculateValue>
		  <FieldName>Source</FieldName>
		  <Expression>&quot;USGS&quot;</Expression>
	  </CalculateValue>
		<CalculateValue>
	 <FieldName>LE90</FieldName>
		  <Expression>59.0</Expression>
	  </CalculateValue>
         <CalculateValue>
		  <FieldName>CE90</FieldName>
		  <Expression>0.0</Expression>
	  </CalculateValue>

		 <CalculateValue>
	 <FieldName>DEM_Type</FieldName>
 		  <Expression>2</Expression>
	  </CalculateValue>
         <CalculateValue>
		 
		  <FieldName>Date_Start</FieldName>
		  <Expression>&quot;1970/01/01&quot;</Expression>
	  </CalculateValue>
		<CalculateValue>
	 <FieldName>Date_End</FieldName>
 		  <Expression>&quot;2010/01/01&quot;</Expression>
	  </CalculateValue>
         <CalculateValue>
		 
		  <FieldName>Source_URL</FieldName>
		  <Expression>&quot;http://pubs.usgs.gov/of/2011/1073/&quot;</Expression>
	  </CalculateValue>

		 <CalculateValue>
	 <FieldName>VerticalDatum</FieldName>
 		  <Expression>&quot;variable&quot;</Expression>
	  </CalculateValue>

	</CalculateValues> 
 

      </Processes>
	
      </MosaicDataset>
  </Workspace>
</Application>